# XO Agent Webhook System Environment Variables
# Copy this file to .env and fill in your actual values

# Required for webhook security
XO_AGENT_SECRET=your_agent_secret_here

# Optional: Webhook URL for agent dispatches
XO_AGENT_WEBHOOK_URL=https://webhook.site/your-webhook-url

# Cloudflare DNS management (for dns.check task)
CLOUDFLARE_API_TOKEN=your_cloudflare_token_here
CLOUDFLARE_ZONE_ID=your_zone_id_here

# Fly.io deployment (for deploy tasks)
FLY_API_TOKEN=your_fly_token_here

# Logging level
LOG_LEVEL=INFO 