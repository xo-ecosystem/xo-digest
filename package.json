{
  "name": "xo-core-dev",
  "version": "1.0.0",
  "private": true,
  "description": "XO Core - Living Universe Infrastructure",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "hardhat test",
    "compile": "hardhat compile",
    "deploy:local": "hardhat run scripts/deploy.js --network localhost",
    "mint:test": "hardhat run scripts/mint.js --network localhost",
    "node": "hardhat node",
    "deploy:polygon": "hardhat run scripts/deploy.js --network polygon",
    "deploy:arbitrum": "hardhat run scripts/deploy.js --network arbitrum",
    "deploy:optimism": "hardhat run scripts/deploy.js --network optimism",
    "deploy:base": "hardhat run scripts/deploy.js --network base",
    "deploy:mumbai": "hardhat run scripts/deploy.js --network polygonMumbai",
    "deploy:arbitrum-goerli": "hardhat run scripts/deploy.js --network arbitrumGoerli",
    "deploy:optimism-goerli": "hardhat run scripts/deploy.js --network optimismGoerli",
    "deploy:base-goerli": "hardhat run scripts/deploy.js --network baseGoerli",
    "deploy:sepolia": "hardhat run scripts/deploy.js --network sepolia",
    "deploy:all-mainnet": "npm run deploy:polygon && npm run deploy:arbitrum && npm run deploy:optimism && npm run deploy:base",
    "deploy:all-testnet": "npm run deploy:mumbai && npm run deploy:arbitrum-goerli && npm run deploy:optimism-goerli && npm run deploy:base-goerli && npm run deploy:sepolia",
    "verify:polygon": "hardhat verify --network polygon",
    "verify:arbitrum": "hardhat verify --network arbitrum",
    "verify:optimism": "hardhat verify --network optimism",
    "verify:base": "hardhat verify --network base",
    "sweep": "node scripts/wallet_sweep_hot_to_cold.js",
    "sign": "./scripts/wallet_sign.sh",
    "verify": "./scripts/wallet_verify.sh",
    "ens:sim": "node scripts/ens_register_batch.js --simulate",
    "ens:commit": "node scripts/ens_register_batch.js --commit-only",
    "ens:reveal": "node scripts/ens_register_batch.js --reveal",
    "ens:daily": "./scripts/ens_daily_publish.sh",
    "ens:set-contenthash": "node scripts/ens_set_contenthash.js",
    "ens:set-subdomain": "node scripts/ens_set_subdomain.js",
    "mb:dev": "next dev -p 3010",
    "digest:build": "node scripts/digest_build_static.js",
    "digest:publish": "./scripts/digest_publish_static.sh"
  },
  "dependencies": {
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "content-hash": "^2.5.2",
    "punycode": "^2.3.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-ethers": "^3.1.0",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.1.0",
    "@openzeppelin/contracts": "^5.4.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.1",
    "ethers": "^6.15.0",
    "hardhat": "^2.26.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "xo",
    "universe",
    "nft",
    "drops",
    "polygon",
    "ethereum"
  ],
  "author": "XO Ecosystem",
  "license": "MIT",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xo-ecosystem/xo-core.git"
  },
  "bugs": {
    "url": "https://github.com/xo-ecosystem/xo-core/issues"
  },
  "homepage": "https://github.com/xo-ecosystem/xo-core#readme"
}
